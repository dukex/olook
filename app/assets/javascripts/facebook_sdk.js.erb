$(document).ready(function() {
  $.ajax({
    url: '//connect.facebook.net/pt_BR/all.js',
    cache: true,
    dataType: 'script',
    success: function(){
    FB.init({
      appId: <%= FACEBOOK_CONFIG['app_id'] %>,
      channelUrl: window.location.protocol + '//' + window.location.host + '/channel.html',
    });
    if(olook){
      olook.facebookCallbacks();
    }
  });
});
