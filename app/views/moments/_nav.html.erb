<nav>
  <ul>
    <% moments.each do |moment| %>
      <li class="<%= moment.id == @moment.id ? 'selected' : nil %> <%= moment.position == 5 ? 'last' : nil %>">
        <div class="box_text">
          <%= link_to moment_path(moment, params: params.except(:action, :controller, :id)) do %>
            <p><%= moment.article %></p>
            <h1><%= moment.name %></h1>
          <% end %>
        </div>
      </li>
    <% end %>
  </ul>
</nav>
