<div class="filter sub">
  <h1 class="subtitle">Salto</h1>
  <ol class="column">
  <% moment_presenter.moment.catalog.heels.each do |heel, label| %>
    <% unless heel.match('0-cm') %>
      <li>
        <%= check_box_tag('heels[]', heel, params[:heels] && params[:heels].include?(heel), :id => heel) %>
        <%= label_tag(label ? label : heel, nil, :for => heel) %>
      </li>
    <% end %>
  <% end %>
  </ol>
</div>






