<div class="filter main">
  <p class="filter_type">Tamanho</p>
  <button type="button" class="clear_filter">( limpar )</button>

    <ol class="column">
      <% moment_presenter.moment.catalog.shoe_sizes.each do |key, value| %>
        <% unless key.zero? %>
          <li>
            <%= check_box_tag('shoe_sizes[]', key, key.to_s == current_user.try(:shoes_size).to_s, :id => "size_#{key}") %>
            <%= label_tag(key, nil, :for => "size_#{key}") %>
          </li>
        <% end %>
      <% end %>
    </ol>

</div>

