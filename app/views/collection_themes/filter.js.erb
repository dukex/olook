<%= render "shared/display_products", :catalog_products => @catalog_products %>
//filter.setMouseOverOnImages();
filter.showAllImages();
<% category_symbol = Category.key_for(params[:category_id].to_i) %>
<% unless category_symbol.blank? %>
  $("#category_filters").html('<%= escape_javascript( render "/shared/filters/color_filters", product_type: category_symbol.to_s ) %>')
  .append('<%= escape_javascript( render partial: "#{category_symbol.to_s}_filters" ) %>')
  .fadeIn("slow");
<% end %>

start_position = <%= params[:price].split("-").first%>;
final_position = <%= params[:price].split("-").last %>;

filter.sliderRange(start_position, final_position);

filter.seeAll();
filter.selectedFilter();
filter.bindObjects();
