<%= render "shared/display_products", :catalog_products => @catalog_products %>
//filter.setMouseOverOnImages();
filter.showAllImages();
<% category_symbol = Category.key_for(params[:category_id].to_i) %>
<% unless category_symbol.blank? %>
  $("#category_filters").fadeIn("slow").html('<%= escape_javascript( render partial: "#{category_symbol.to_s}_filters") %>');
<% end %>
start_range = <%= Product.only_visible.bags.joins(:variants).order("variants.retail_price ASC").first.retail_price.to_s %>;
final_range = <%=Product.only_visible.bags.joins(:variants).order("variants.retail_price DESC").first.retail_price.to_s %>;
filter.sliderRange(start_range, final_range);
filter.seeAll();
filter.selectedFilter();
filter.bindObjects();
