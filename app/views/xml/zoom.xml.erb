<?xml version="1.0" encoding="UTF-8"?>
<produtos>
<% @products.each do |product| %>
<produto>
<codigo><%= product.id %></codigo>
<nome><%= product.name %></nome>
<descricao><%= product.description %></descricao>
<preco_de><%= product.price %></preco_de>
<preco><%= product.retail_price %></preco>
<nparcela>1</nparcela>
<vparcela></vparcela>
<url><%= product.product_url(:utm_source => "zoom") %></url>
<url_imagem><%= product.pictures.last.image %></url_imagem>
<departamento></departamento>
<subdepartamento>Computadores</subdepartamento>
<isbn/>
<editora/>
<autor/>
<mpc></mpc>
<ean></ean>
<sku></sku>
</produto>
<% end %>
</produtos>
