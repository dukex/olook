<div id="invite_friends">
  <h1>Envie convites pessoais para suas amigas</h1>
  <%= link_to "Ver todas amigas", "javascript:void(0);", :class => "all_friends" %>
  <div class="list_mask">
    <ul>
      <% not_registred_friends.each do |friend| %>
      <li>
        <%= link_to image_tag(user_avatar(friend, "square"), :title => friend.name), "javascript:void(0);" %>
      </li>
      <% end %>
    </ul>
  </div>
</div>
