<div id="pixels"/>
<script type="text/javascript">
  dataLayer.push({
    'UserEmail': '<%= email %>',
    'zanpid': '<%= @zanpid %>',
    'RegisterSource': '<%= raw register_source %>'
  });


  if ('<%=partner%>' == 'adlead') {
    adlead = function() {
      var img = document.createElement("img");
      img.src = "//tru.webelapp.com/adtckrg.php?idc=60094&idctr=6&email=<%= email %>&idp=0"

      var src = document.getElementById("pixels");
      src.appendChild(img);
    };

    window.addEventListener('load', adlead);
  }


</script>
