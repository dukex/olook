<div class="dialog product_login">
  <%= link_to "X", "javascript:void(0);", :id => "close_dialog" %>
  <div id="login_modal">
    <div id="register_login">
      <div class="login">
        <h1>Já faz parte?</h1>
        <%= render "users/sessions/form_user" %>
      </div>
      <div class="register">
        <h1>Faça seu cadastro</h1>
        <p>É rápido, fácil e grátis.</p>
        <%= link_to "Resonda e cadastre-se", new_survey_path %>
      </div>
    </div>
  </div>
</div>
