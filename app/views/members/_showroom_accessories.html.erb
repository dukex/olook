    <section class="container_product" id="accessories_container">
      <h1 class="title">Acessórios de <%= showroom_presenter.collection_name %></h1>
      <div class="type_list">
        <div class="products_list best">
          <ul>
            <%= showroom_presenter.display_accessories (0..4) %>
          </ul>
          <%= link_to "Veja os acessórios que quase entraram para sua vitrine", "javascript:void(0);", :class => "more accessories", :rel => "almost" %>
        </div>
        <div class="products_list almost">
          <ul>
            <%= showroom_presenter.display_accessories (5..9) %>
          </ul>
          <%= link_to "Veja mais acessórios", "javascript:void(0);", :class => "more all_accessories", :rel => "all" %>
        </div>
        <div class="products_list all">
          <ul>
            <%= showroom_presenter.display_accessories (10..100) %>
          </ul>
        </div>
      </div>
    </section>
