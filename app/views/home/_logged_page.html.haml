#looks
  %h3
    Meus Looks
    =link_to "ver meu estilo", profile_url
  .mylook
    - @showroom_presenter.looks.each_with_index do |look,idx|
      .look{class: ('last' if idx == 3)}
        = link_to product_seo_url(look.product.seo_path, onclick: track_event("LoggedHome", "ClickLook", "#{look.product_id}")) do
          %span.look_thumbnail{data: {product: look.front_picture.showroom.url, backside_picture: look.full_look_picture.showroom.url }}
  #see_too
    %h4
      Veja TambÃ©m
    %ul#carousel.elastislide-list
      - @showroom_presenter.products.each do |product|
        %li
          = link_to image_tag(product.front_picture.try(:image_url, :showroom)), product_seo_url(product.seo_path), onclick: track_event("LoggedHome", "ClickCarouselProduct", "#{product.id}")
          %span= number_to_currency(product.retail_price)
          = link_to "Ver Mais", product_seo_path(product.seo_path), :class => 'seeMore'
