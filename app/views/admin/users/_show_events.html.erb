<h3>Eventos</h3>
<table class="table">
  <tr>
    <th>Tipo</th>
    <th>Descrição</th>
    <th>Quando</th>
  </tr>

<% user.events.each do |event| %>
  <tr>
    <td><%= event.event_type_humanize %></td>
    <td><%= event.description %></td>
    <td><%= event.created_at.to_s(:short) %></td>
  </tr>
<% end %>
</table>

