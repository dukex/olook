<h2>User</h2>
<div class="inner">
  <p>
    <b>First Name:</b>
    <%= @user.first_name %>
  </p>
  <p>
    <b>Last Name:</b>
    <%= @user.last_name %>
  </p>
  <p>
    <b>E-mail:</b>
    <%= @user.email %>
  </p>
  <p>
    <b>CPF:</b>
    <%= @user.cpf %>
  </p>
  <p>
    <b>Invite?</b>
    <%= @user.is_invited? ? 'Yes' : 'No' %>
  </p>
  <p>
    <b>Created At:</b>
    <%= @user.created_at %>
  </p>
</div>

<% content_for :secondary_navigation do %>
  <li><%= link_to 'Edit', edit_admin_user_path(@user) %></li>
  <li><%= link_to 'Back', admin_users_path %></li>
<% end %>
