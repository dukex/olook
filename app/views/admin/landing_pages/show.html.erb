<div class="inner">
  <p id="notice"><%= notice %></p>

  <p>
    <b>Título:</b>
    <%= @landing_page.page_title %>
  </p>

  <p>
    <b>URL:</b>
    <%= link_to @landing_page.page_url,  landing_url(@landing_page.page_url) %>
  </p>

  <p>
    <b>Imagem:</b>
    <%= image_tag @landing_page.page_image.thumb.try(:url) %>
  </p>

  <p>
    <b>Button url:</b>
    <%= @landing_page.button_url %>
  </p>

  <p>
    <b>Button alt:</b>
    <%= @landing_page.button_alt %>
  </p>

  <p>
    <b>Button image:</b>
    <%= image_tag @landing_page.button_image.thumb.try(:url) %>
  </p>

  <p>
    <b>Habilitado:</b>
    <%= @landing_page.enabled ? "Sim" : "Não"%>
  </p>

  <p>
    <b>Mostrar header:</b>
    <%= @landing_page.show_header ? "Sim" : "Não" %>
  </p>

  <p>
    <b>Mostrar footer:</b>
    <%= @landing_page.show_footer ? "Sim" : "Não"%>
  </p>

  <%= link_to 'Editar', edit_admin_landing_page_path(@anding_page) %> |
  <%= link_to 'Voltar', admin_landing_pages_path %>
</div>
