<h1>Listing shipping_companies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Erp code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shipping_companies.each do |shipping_company| %>
  <tr>
    <td><%= shipping_company.name %></td>
    <td><%= shipping_company.erp_code %></td>
    <td><%= link_to 'Show', admin_shipping_company_path(shipping_company) %></td>
    <td><%= link_to 'Edit', edit_admin_shipping_company_path(shipping_company) %></td>
    <td><%= link_to 'Destroy', admin_shipping_company_path(shipping_company), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

